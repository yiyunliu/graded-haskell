Local Open Scope grade_scope.

Definition labels : context -> econtext :=
  map (fun '(u , s) => u).

Definition subst_ctx (a : tm) (x:var) : context -> context :=
  map (fun '(g, A) => (g, (subst_tm_tm a x A))).

Definition squash_ctx (ψ : grade) : context -> context :=    
  map (fun '(ψ0, A) => (ψ ⤓ ψ0, A)).

