formula :: 'formula_' ::=
  | judgement                ::   :: judgement

  | x : psi in P               ::   :: inP
    {{ coq binds [[x]] [[psi]] [[P]] }}

  | x : psi A in W               ::   :: inW
    {{ tex [[x]]\! :^{[[psi]]}\![[A]] \in [[ W ]] }}
    {{ coq binds [[x]] ([[psi]], [[A]]) [[W]] }}
  | x in fv a             ::   :: used
    {{ coq [[x]] `in` fv_tm_tm_tm [[a]] }}
    {{ tex [[x]]\ [[in]]\ [[fv]]\ [[a]] }}
  | x notin fv a             ::   :: fresh
    {{ coq [[x]] `notin` fv_tm_tm_tm [[a]] }}
  | x notin dom W            ::   :: notInGx
    {{ coq ~ AtomSetImpl.In [[x]] (dom [[W]]) }}
  | x notin dom D            ::   :: notInDx
    {{ coq ~ AtomSetImpl.In [[x]] (dom [[D]]) }}
  | x notin dom P            ::   :: notInPx
    {{ coq ~ AtomSetImpl.In [[x]] (dom [[P]]) }}
  | x notin dom H            ::   :: notInHX
    {{ coq ~ AtomSetImpl.In [[x]] (dom [[H]]) }}
    {{ tex [[ x ]] \notin [[ dom ]] \: [[ H ]] }}
  | a = b                    ::   :: equal
    {{ coq [[a]]=[[b]] }}
  | a = psi b                ::   :: equalQ
    {{ tex [[ a ]] =_{[[ psi ]]} [[ b ]] }}

  | W1 = W2                  ::   :: equalctx
    {{ coq ([[W1]] = [[W2]]) }}

  | D1 = D2                  ::   :: equalD
    {{ coq ([[D1]] = [[D2]]) }}

  | psi1 <= psi2              ::   :: subrho
    {{ coq ([[psi1]] ≤ [[psi2]]) }}
  | psi1 < psi2               ::   :: ltrho
    {{ coq ([[psi1]] < [[psi2]]) }}


  | True                     ::   :: True
    {{ coq True }}
  | False                    ::   :: False
    {{ coq False }}
  | not formula              ::   :: not
    {{ coq not ([[formula]]) }}
    {{ tex \neg [[formula]] }}
  | formula1 /\ formula2     ::   :: and
    {{ coq [[formula1]] /\ [[formula2]] }}
  | formula1 \/ formula2     ::   :: or
    {{ coq [[formula1]] \/ [[formula2]] }}
  | formula1 implies formula2     ::   :: implies
    {{ coq [[formula1]] -> [[formula2]] }}
    {{ tex [[formula1]]\ [[implies]]\ [[formula2]] }}
  | formula1 iff formula2     ::   :: iff
    {{ coq [[formula1]] <-> [[formula2]] }}

  | ( formula )              ::   :: Paren
    {{ coq ([[formula]]) }}

  | uniq W                   ::   :: uniqW
    {{ coq uniq [[W]] }} {{ tex }}

  | uniq P                   ::   :: uniqP
    {{ coq uniq [[P]] }} {{ tex }}

  | [ formula ] ::    :: Suppress
    {{ tex \suppress{[[formula]]} }}
         {{ coq [[formula]] }}

  | nolc A                  :: M :: nolcA
    {{ tex }}
    {{ coq True }}

  | nolc C                  :: M :: nolcC
    {{ tex }}
    {{ coq True }}


  | ctx D W :: M :: ctx
    {{ coq ctx [[D]][[W]] }}
    {{ tex [[D]]\vdash[[W]] }}

  | |- a ~>* b ::  :: multistep

  | W |- a : A :: :: Typ {{ tex [[W]] \vdash[[a]]:[[A]] }}

  | axiom s1 s2 ::  :: axiom
     {{ coq axiom [[s1]] [[s2]] }}
  | rule_pi s1 s2 s3 ::   :: rule_pi
     {{ coq rule_pi [[s1]] [[s2]] [[s3]] }}
  | rule_sig s1 s2 s3 ::   :: rule_sig
     {{ coq rule_sig [[s1]] [[s2]] [[s3]] }}


